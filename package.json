    "dev:twitter-media-filter": "vite --mode twitter-media-filter",
    "dev:twitter-thread-copier": "vite --mode twitter-thread-copier",
    "dev:twitter-wide-layout-fix": "vite --mode twitter-wide-layout-fix",
    "dev:youtube-info-copier": "vite --mode youtube-info-copier",
