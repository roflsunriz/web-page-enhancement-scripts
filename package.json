    "eslint": "^9.39.1",
    "prettier": "^3.7.3",
    "react-refresh": "^0.18.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vite-plugin-monkey": "^7.1.5",
    "zod": "^4.3.6"
